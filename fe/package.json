  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "jss": "^10.4.0",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.7"
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=local REACT_APP_MAIN_URL=http://localhost:9090 REACT_APP_ASSETS_ROOT=http://localhost:5000 react-scripts start",
    "build": "cross-env REACT_APP_ENV=prod REACT_APP_MAIN_URL=reactale.com REACT_APP_ASSETS_ROOT=https://webapp.reactale.com react-scripts build",
    "build-dev": "cross-env REACT_APP_ENV=dev REACT_APP_MAIN_URL=reactale.site REACT_APP_ASSETS_ROOT=https://webapp.reactale.site react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.7",
    "handlebars": "^4.7.6",
    "typescript": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
